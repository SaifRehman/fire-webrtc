{
  "name": "fire-webrtc",
  "version": "0.0.7",
  "description": "webcompenent for p2p vedio chat using webrtc and a signalling server using firebase",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fire-rtc/fire-rtc.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.7",
    "@stencil/sass": "^1.0.1"
  },
  "license": "MIT",
  "dependencies": {
    "firebase": "^6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaifRehman/fire-webrtc"
},
"keywords": ["webrtc","p2p","stencil","webcomponent"]
}
